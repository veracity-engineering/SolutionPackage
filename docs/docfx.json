{
	"metadata": [
		{
			"src": [
				{
					"files": [
						"**/*.csproj"
					],
					"exclude": [
						"**/bin/**",
						"**/obj/**",
						"*.Tests/**",
						"*.Demo/**"
					],
					"src": "../src"
				}
			],
			"dest": "api"
		}
	],
	"build": {
		"content": [
			{
				"files": [
					"articles/**.md",
					"articles/toc.yml",
					"api/**.md",
					"api/**.yml",
					"toc.yml",
					"*.md"
				]
			}
		],
		"resource": [
			{
				"files": [
					"images/**"
				]
			}
		],
		"dest": "_site",
		"globalMetadataFiles": [],
		"fileMetadataFiles": [],
		"template": [
			"default"
		],
		"postProcessors": [],
		"markdownEngineName": "markdig",
		"noLangKeyword": false,
		"keepFileLink": false,
		"cleanupCacheHistory": false,
		"disableGitFeatures": false
	}
}
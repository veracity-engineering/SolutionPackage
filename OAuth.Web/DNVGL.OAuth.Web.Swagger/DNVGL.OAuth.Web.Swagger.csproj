		<Authors>Eric Liu &lt;li.eric.liu@dnv.com&gt;</Authors>
		<Company>DNV</Company>
		<GenerateDocumentationFile>True</GenerateDocumentationFile>
		<Version>1.1.2</Version>
	</PropertyGroup>

	<ItemGroup>
		<PackageReference Include="Swashbuckle.AspNetCore.SwaggerGen" Version="6.3.1" />

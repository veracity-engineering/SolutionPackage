<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFrameworks>netcoreapp3.1;net6.0</TargetFrameworks>
		<LangVersion>latest</LangVersion>
		<Nullable>enable</Nullable>
		<Authors>Eric Liu &lt;li.eric.liu@dnv.com&gt;</Authors>
		<Company>DNV</Company>
		<GenerateDocumentationFile>True</GenerateDocumentationFile>
		<Version>1.1.6</Version>
	</PropertyGroup>
	
	<ItemGroup Condition=" '$(TargetFramework)' == 'netcoreapp3.1'">
		<PackageReference Include="Microsoft.AspNetCore.Authentication.JwtBearer" Version="3.1.30" />
		<PackageReference Include="Microsoft.AspNetCore.Authentication.OpenIdConnect" Version="3.1.30" />
		<PackageReference Include="Microsoft.AspNetCore.Authorization" Version="3.1.30" />
	</ItemGroup>

	<ItemGroup Condition=" '$(TargetFramework)' == 'net6.0'">
		<PackageReference Include="Microsoft.AspNetCore.Authentication.JwtBearer" Version="6.0.10" />
		<PackageReference Include="Microsoft.AspNetCore.Authentication.OpenIdConnect" Version="6.0.10" />
		<PackageReference Include="Microsoft.AspNetCore.Authorization" Version="6.0.10" />
	</ItemGroup>

	<ItemGroup>
		<ProjectReference Include="..\..\OAuth\DNV.OAuth.Core\DNV.OAuth.Core.csproj" />
	</ItemGroup>

	<PropertyGroup>
		<DocumentationFile></DocumentationFile>
		<NoWarn>1701;1702;S101;CS1591</NoWarn>
	</PropertyGroup>

</Project>